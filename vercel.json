{
  "functions": {
    "api/app.py": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "api/**"
    }
  },
  "buildCommand": "pip install pipenv && pipenv install --deploy && yarn --cwd www/libx install && yarn --cwd www/libx build",
  "outputDirectory": "www/libx/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/www/libx/dist/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/app.py"
    }
  ]
}
